<div id="all2">
  <h1 id="title">MOON IPTV</h1>
  <%= form_for :session do |s| %>
    <div class="form-group font-weight-bold">
      <label for="exampleInputEmail1">Username</label>
      <%= s.text_field :username, class: "form-control" %>
    </div>
    <div class="form-group font-weight-bold">
      <label for="exampleInputPassword1">Password</label>
      <%= s.password_field :password, class: "form-control" %>
    </div>
    <%= s.submit "Login", class:"btn btn-dark btn-lg" %>
  <% end %>
</div>