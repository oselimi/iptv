<%provide(:title, "MOON IPTV")%>
<img src="https://media.indiedb.com/images/games/1/51/50676/moon-transparent.png" id="home"  width="250px" height="250px">
<h1 class="home">Welcome to MOON IPTV</h1>
  <p class="clock">
    <%= Time.now %>
  </p>
<% if logged_in? %>
  <div class="ct">
    <%= link_to product_path(current_user) do %>
      <img id="tv" src="https://img.icons8.com/pastel-glyph/64/000000/tv.png" width="120px" height="120px">
    <% end %>
    <%= link_to user_path(current_user) do %>
      <img id="tv" src="https://img.icons8.com/ios-filled/50/000000/checked-user-male.png" width="120px" height="120px">
    <% end %>
  </div>
<% end %>

